{
  "manifest_version": 1,
  "type": "WebApp",
  "meta": {
    "name": "Overwolf Next.js Starter",
    "author": "OskarD",
    "version": "0.0.1",
    "minimum-overwolf-version": "0.160.0",
    "description": "A Next.js starter for native Overwolf apps",
    "dock_button_title": "Starter - Next.js",
    "icon": "icons/IconMouseOver.png",
    "icon_gray": "icons/IconMouseNormal.png",
    "launcher_icon": "icons/desktop-icon.ico",
    "window_icon": "icons/IconMouseOver.png"
  },
  "permissions": [
    "Extensions",
    "Streaming",
    "GameInfo",
    "Profile",
    "Hotkeys",
    "DesktopStreaming",
    "FileSystem",
    "Clipboard"
  ],
  "data": {
    "start_window": "background",
    "hotkeys": {
      "nextjs_starter_app_showhide": {
        "title": "Show/Hide In-Game Window",
        "action-type": "toggle",
        "default": "Ctrl+F"
      }
    },
    "protocol_override_domains": {
      "googlesyndication": "http"
    },
    "externally_connectable": {
      "matches": [
        "http://*.overwolf.com",
        "https://*.overwolf.com",
        "https://*.google-analytics.com",
        "http://*.google-analytics.com",
        "https://www.googleapis.com"
      ]
    },
    "force_browser": "user",
    "windows": {
      "background": {
        "file": "index.html",
        "background_optimization": false,
        "is_background_page": true
      },
      "in_game": {
        "file": "index.html",
        "transparent": true,
        "resizable": false,
        "in_game_only": true,
        "show_in_taskbar": false,
        "keep_window_location": true,
        "override_on_update": true,
        "block_top_window_navigation": true,
        "popup_blocker": true,
        "mute": true,
        "focus_game_takeover": "ReleaseOnHidden",
        "focus_game_takeover_release_hotkey": "toggle-front-app",
        "size": {
          "width": 3440,
          "height": 1440
        },
        "min_size": {
          "width": 3440,
          "height": 1440
        }
      }
    },
